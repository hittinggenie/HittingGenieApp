<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HittingGenie.VideoPages.PracticeVideo"
             
             Title="">
    <VerticalStackLayout>
        <Label x:Name="Lbl1" Text="" />
         <Image x:Name="logo" Source="genie1.png" WidthRequest="75" HeightRequest="75"/>
        <Label x:Name="Lbl2" Text="When they move, you move." />
        <Label x:Name="Lbl3" Text="Watch this video to confirm we are on the same page" />
       
          <WebView x:Name="videoPlayer" WidthRequest="0" HeightRequest="250" />
        <Button x:Name="VideoWatched" Text="I Watched the Video" Clicked="VideoWatched_Clicked" />
    </VerticalStackLayout>
</ContentPage>
